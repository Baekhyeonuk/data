<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js">
</script>
<script>


function j_test_gogo(o, n){


$(o).animate({left:-500,top:220}, n, function(){
$(o).hide();
});

}

</script>
</head>
<body>
<div class="css_test">
<img onclick="j_test_gogo(this, 3000)" src="http://superkts.com/img/css/car0194.gif" /><br />
